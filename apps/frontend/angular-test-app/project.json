{
  "name": "angular-test-app",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/frontend/angular-test-app/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular/build:application",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/frontend/angular-test-app",
        "browser": "apps/frontend/angular-test-app/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/frontend/angular-test-app/tsconfig.app.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/frontend/angular-test-app/public"
          }
        ],
        "styles": ["apps/frontend/angular-test-app/src/styles.css"]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "4kb",
              "maximumError": "8kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular/build:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "angular-test-app:build:production"
        },
        "development": {
          "buildTarget": "angular-test-app:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular/build:extract-i18n",
      "options": {
        "buildTarget": "angular-test-app:build"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/frontend/angular-test-app/jest.config.ts"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "angular-test-app:build",
        "staticFilePath": "dist/apps/frontend/angular-test-app/browser",
        "spa": true
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --project apps/frontend/angular-test-app/tsconfig.app.json"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "biome check apps/frontend/angular-test-app/src"
      }
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "biome check --write apps/frontend/angular-test-app/src"
      }
    },
    "check": {
      "executor": "nx:run-commands",
      "dependsOn": ["typecheck"],
      "options": {
        "commands": ["nx typecheck angular-test-app", "nx lint angular-test-app"],
        "parallel": false
      }
    }
  }
}
